{
  "kind": "collectionType",
  "collectionName": "food_sizes",
  "info": {
    "singularName": "food-size",
    "pluralName": "food-sizes",
    "displayName": "Food Sub Group"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": false
    },
    "dishes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::dish.dish",
      "mappedBy": "food_sub_group"
    },
    "desc": {
      "type": "text"
    },
    "img": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "positionOfImg": {
      "type": "enumeration",
      "default": "left",
      "enum": [
        "left",
        "right"
      ]
    },
    "templateOfImg": {
      "type": "enumeration",
      "enum": [
        "round16",
        "roundTop",
        "roundTopRight"
      ]
    }
  }
}
